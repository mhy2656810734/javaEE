package thread;

/**
 * 使用匿名内部类,继承Thread类,重写run方法
 * @author 26568
 * @date 2022-11-23 15:42
 */
public class ThreadDemo3 {
    public static void main(String[] args) {
        Thread t = new Thread() {
            @Override
            public void run() {
                System.out.println("hello thread");
            }
        };
        t.start();
    }
}
